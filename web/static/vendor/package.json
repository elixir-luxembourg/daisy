{
  "name": "elixir-daisy",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:css && npm run build:css-min",
    "watch": "concurrently --kill-others --names \"css\"  \"npm run watch:css\"",
    "watch:css": "npm run build:css && npm run build:css -- --watch",
    "build:css": "node-sass --importer=node_modules/node-sass-tilde-importer ../css/daisy.scss  ../css/daisy.css",
    "build:css-min": "cssnano ../css/daisy.css ../css/daisy.min.css"
  },
  "repository": {
    "type": "git",
    "url": "https://git-r3lab.uni.lu/elixir/daisy"
  },
  "author": "LCSB",
  "license": "Apache-2.0",
  "dependencies": {
    "@ds-wizard/integration-widget-sdk": "^0.2.0",
    "bootstrap": "4.2.1",
    "bootstrap-material-design": "4.1.1",
    "datatables.net": "1.10.19",
    "datatables.net-dt": "1.10.19",
    "jquery": "3.4.1",
    "jquery-ui-dist": "1.12.1",
    "js-cookie": "2.2.0",
    "jsrender": "0.9.90",
    "moment": "2.22.1",
    "npm": "6.13.7",
    "popper.js": "1.15.0",
    "select2": "4.0.6-rc.1",
    "select2-bootstrap-theme": "0.1.0-beta.10"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "cssnano-cli": "1.0.5",
    "js-yaml": "3.13.1",
    "node-sass": "4.13.1",
    "node-sass-tilde-importer": "1.0.2"
  }
}
